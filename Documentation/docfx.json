{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "Assets/**/*.cs"
          ],
          "exclude": [
            "Assets/Plugins/**",
            "Assets/TextMesh Pro/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "Project Crovus",
      "_appFooter": "Project Crovus",
      "_appLogoPath": "resources/LogoIcon.png",
      "_appFaviconPath": "resources/LogoIcon.png",
      "_enableNewTab": true,
      "_enableSearch": true
    },
    "content": [
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      },
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "manual/**.md",
          "manual/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "overwrite": [
      {
        "src": "..",
        "files": [
          "Assets/Scripts/**/*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "resources/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/darkfx",
      "templates/lightbox-featherlight"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}